<Window x:Class="Pacman.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pacman"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="Pacman" Height="600" Width="800">
    
        <Grid>
        <Rectangle x:Name="Pacman" HorizontalAlignment="Left" Height="50" Stroke="Black" VerticalAlignment="Top" Width="50" Margin="0,0,0,0" RadiusY="26" RadiusX="26">
            <Rectangle.Fill>
                <RadialGradientBrush>
                    <GradientStop Color="#FFF4EE14" Offset="1"/>
                    <GradientStop Color="#FFE5DF2C"/>
                </RadialGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
    </Grid>
</Window>
